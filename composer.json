{
    "name": "james.xue/packet",
    "description": "代码格式化、静态分析等",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "static analysis",
        "composer",
        "unused",
        "php-parser",
        "rector",
        "fixer",
        "pint"
    ],
    "authors": [
        {
            "name": "xiaoxuan6",
            "email": "1527736751@qq.com"
        }
    ],
    "homepage": "https://github.com/xiaoxuan6/packet.git",
    "support": {
        "issues": "https://github.com/xiaoxuan6/packet/issues",
        "source": "https://github.com/xiaoxuan6/packet"
    },
    "require": {
        "php": "^8.1",
        "illuminate/collections": "^10.48",
        "symfony/finder": "^6.4",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.42",
        "friendsofphp/php-cs-fixer": "^3.52",
        "icanhazstring/composer-unused": "^0.8.11",
        "rector/rector": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/packet"
    ],
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "fix": [
            "@rector",
            "@pint"
        ],
        "pint": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --using-cache=no",
        "rector": "vendor/bin/rector process --config=rector.php --no-diffs --ansi",
        "unused": "vendor/bin/composer-unused --ansi"
    }
}
